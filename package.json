{
  "private": false,
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && yarn build:prisma && yarn build:remix",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build:prisma": "prisma generate --data-proxy",
    "build:remix": "remix build",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"yarn dev:css \" \"remix build && run-p \\\"dev:*\\\"\"",
    "start": "remix-serve build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@reach/dialog": "^0.18.0",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@remix-validated-form/with-zod": "^2.0.5",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.5",
    "luxon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.0",
    "remix-auth": "^3.3.0",
    "remix-auth-form": "^1.2.0",
    "remix-typedjson": "^0.1.6",
    "remix-validated-form": "^4.6.4",
    "tiny-invariant": "^1.3.1",
    "ulid": "^2.3.0",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/luxon": "^3.1.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
